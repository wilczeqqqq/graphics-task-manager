<div id="<%= dom_id artist %>">
  <p>
    <strong>Login:</strong>
    <%= artist.login %>
  </p>

  <p>
    <strong>Password digest:</strong>
    <%= artist.password_digest %>
  </p>

  <p>
    <strong>Nickname:</strong>
    <%= artist.nickname %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= artist.bio %>
  </p>

  <p>
    <strong>Preffered style:</strong>
    <%= artist.preferred_style %>
  </p>

  <p>
    <strong>Token:</strong>
    <%= artist.token %>
  </p>

</div>
